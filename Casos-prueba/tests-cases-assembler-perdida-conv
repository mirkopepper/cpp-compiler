assembler

integer i;
allow double to integer;

&&bloque ejecutable
{
    i:=_d2.5;
    if(i=_i2)
        print("es igual a 2");
    else
        if(i=_i3)
            print("es igual a 3");
        else
            print("chequear codigo!!");
        endif
    endif
    print("si se imprime esto, no funciona el chequeo por perdida de informacion en conversiones");
}
