assembler

&&sentencias de declaracion de matrices
double matrix matDou [_i1][_i1]; &&@C esto es una anotacion de matriz arrobaF

&&bloque ejecutable
{

    matDou[_i0][_i0]:=_d1.;
    matDou[_i0][_i1]:=matDou[_i0][_i0]+_d1.;
    matDou[_i1][_i0]:=matDou[_i0][_i1]*_d3.;
    matDou[_i1][_i1]:=matDou[_i1][_i0]/matDou[_i0][_i1];

    if(matDou[_i0][_i0]=_d1.)
        print("cargo 1 en 0,0 !!!");
    endif
    if(matDou[_i0][_i1]=_d2.)
        print("cargo 2 en 0,1 !!!");
    endif
    if(matDou[_i1][_i0]=_d6.)
        print("cargo 6 en 1,0 !!!");
    endif
    if(matDou[_i1][_i1]=_d3.)
        print("cargo 3 en 1,1 !!!");
    endif

    print("termino");
}
